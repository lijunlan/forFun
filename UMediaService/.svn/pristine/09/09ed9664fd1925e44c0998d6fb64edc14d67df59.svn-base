<script type="text/javascript">
	$(document)
			.ready(
					function() {
						if (getCookie("user_id") == null) {
							alert("请先登录！");
							self.location = 'login.html';
						}
						$
								.ajax({
									cache : true,
									type : "POST",
									url : "/UMediaService/manage",
									data : "{'style':'user','method':'get_user_info','user_id':'"
											+ getCookie("user_id") + "'}",
									async : false,
									error : function(request) {
										//alert("Connection error");
									},
									success : function(data, textStatu) {
										var json = eval("(" + data + ")");
										$("#username").html(
												getValueFromJson(json,
														"username"));
										$("#name2").html(
												getValueFromJson(json, "name"));
										$("#name").val(
												getValueFromJson(json, "name"));
										$("#address").html(
												getValueFromJson(json,
														"address"));
										$("#sex").html(
												getValueFromJson(json, "sex"));
										$("#realname").html(
												getValueFromJson(json,
														"realname"));
										$("#birthday").html(
												getValueFromJson(json,
														"birthday"));
										$("#bloodtype").html(
												getValueFromJson(json,
														"bloodType"));
										$("#introduce").html(
												getValueFromJson(json,
														"introduce"));
										$("#phone")
												.html(
														getValueFromJson(json,
																"phone"));
										$("#email")
												.html(
														getValueFromJson(json,
																"email"));
										$("#qq").html(
												getValueFromJson(json, "qq"));
									}
								});
					});
	function logout() {
		delCookie("user_id");
		self.location = 'login.html';
	}
</script>
