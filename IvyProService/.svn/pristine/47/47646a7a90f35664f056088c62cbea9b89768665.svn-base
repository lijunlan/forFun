@charset "utf-8";

/*content*/
#content{
	width: 1000px;
	margin-right: auto;
	margin-left: auto;
}
#title{
	width: 100%;
	border-bottom: 2px solid #cccccc;
	font-size: 23px;
	padding: 5px;
	margin-top: 30px;
	color: #363636;
}
#title>p>span{
	font-size: 15px;
	color: #acacac;

}
#subTitle>p{
	font-size: 25px;
	font-weight: bold;
	color: #363636;
	margin-top: 20px;
	text-align: center;
}
.column{
	width: 100%;
}
.columnHead{
	height: 68px;
	width: 100%;
	margin-top: 30px;
	line-height: 68px;
	float: left;
}
.columnHead>p{
	font-size: 20px;
	color: #363636;
	float: left;
	margin-left: 20px;
}
.columnHead>p>span{
	margin-left: 10px;
	font-size: 15px;
	color: #b7b7b7;
}
.numCircle{
	height: 50px;/*行高68px*/
	width: 50px;
	margin-top: 9px;
	margin-bottom: 9px;
	line-height: 50px;
	border-radius: 25px;
	background: #e95e59;
	text-align: center;
	float: left;
}
.numCircle>p{
	font-size: 30px;
	color: white;
	font-weight: bolder;

}
.unfoldBt{
	width: 75px;
	height: 16px;
	line-height: 16px;
	font-size: 16px;
	color:#e95e59;
	float: right;
	background: url(../image/fold.png) no-repeat;
	background-position: 53px 1px;
	cursor: pointer;
	margin-top: 26px;
}
.unfoldBt.click{
	background: url(../image/unfold.png) no-repeat;
	background-position: 53px 1px;
}
.columnBody{
}
.memberInfo{
	font-size: 16px;
	color: #b7b7b7;
	text-align: center;
	margin-top: 30px;
    margin-right: 81px;
    margin-left: 81px;
	float: left;
}
.memberInfo>h4{
	font-size: 20px;
	color: #363636;
	margin-top: 10px;
	margin-bottom: 10px;
}
.pic{
    position: relative;
	width: 338px;
	height: 450px;
}
/*floating window*/
#float{
	background-color: rgba(247,247,247,0.7);
    width: 300px;
    height: 140px;
    position: fixed;
    right: 0px;
    top: 40%;
}
#float>img{
	float: left;
    width: 106px;
    height: 106px;
    margin: 17px;
}
#float>p{
	float: left;
	color: black;
	margin-bottom: 3px;
}
#float>p:nth-of-type(1){
	margin-top: 17px;
}
#closeFloat{
	position: absolute;
	height: 21px;
	width: 21px;
	background: url(../image/close_float.png);
	z-index: 2;
	left: -5px;
	top: -5px;
}